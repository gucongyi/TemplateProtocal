<?xml version="1.0" encoding="UTF-8"?>
<msgs package="" name="ProtoServerClass">
    <class name="GoodInfoSign" des="物品信息">
        <field name="goodId" type="int" des="物品ID"/>
        <field name="signState" type="EnumSignInfo" des="签到状态"/>
        <field name="whatDay" type="int" des="第几天"/>
    </class>
    <class name="ProtoServerBaseClass">
    </class>
    <class name="TestInfo" des="" extends="ProtoServerBaseClass">
        <field name="test1" type="int" des="" />
        <field name="test2" type="long" des="" />
        <field name="test3" type="string" des="" />
    </class>
    <req name="CSSignInfo" resp="RespSignInfo" des="服务器回给客户端的" extends="ProtoServerBaseClass">
    </req>

    <req name="CSGetInfo" resp="RespInfo" des="客户端向服务器请求的" extends="ProtoServerBaseClass">
        <field name="day" type="int" des="" />
    </req>
    <resp name="RespInfo" type="">
    	<field name="testInfo" type="TestInfo" des="" />
    </resp>
	<resp name="RespSignInfo" type="">
    	<field name="listGoodInfoSign" type="List[GoodInfoSign]" des="" />
        <field name="timeBegin" type="long" des="" />
        <field name="timeEnd" type="long" des="" />
    </resp>
	<resp name="RespInfo" type="notice">
    	<field name="testInfo" type="TestInfo" des="" />
    </resp>
    <enum name="EnumSignInfo" des="物品状态">
        <field name="NormalSign" />
        <field name="UnSign" />
        <field name="RepairSign" des="补签" />
    </enum>

    <enum name="EnumGoodType" des="商城主分类">
        <field name="Furniture" des="家具" />
        <field name="Clothing" des="时装" />
        <field name="Heartbeat" des="心动值" />
    </enum>

    <enum name="EnumTitleType" des="">
        <field name="TitleType1" des="" value="0" />
        <field name="TitleType2" des="" />
        <field name="TitleType3" des="" />
        <field name="TitleType4" des="" />
        <field name="TitleType5" des="" />
    </enum>

    <!-- 所有成功均返回value:0 -->
    <class name="ProxyDicCode" type="CodeError">
        <field name="Success" msg="成功" value="0" />

        <field name="User_UnLogin" msg="用户未登录"  value="1" />
        <field name="Sign_ParamException" msg="签到参数异常"  value="101" />
        <field name="Sign_Signed" msg="已签到"  value="102" />
        <field name="Sign_RepairSigned" msg="已补签"  value="103" />
        <field name="Sign_NotInSignTime" msg="不在签到时间范围内"  value="104" />
    </class>
</msgs>