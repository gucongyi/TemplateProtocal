    </req>
	
	<req name="CSFriendClearVisitRecord" resp="SCFriendClearVisitRecord" des="请求清除访问列表" extends="MsgRequest">
	</req>
    <!-- 好友 -->

    <!-- 商城 -->
	<req name="CSShopInfo" resp="SCShopInfo" des="请求商城信息" extends="MsgRequest">
    </req>
	
